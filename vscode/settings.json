{
    "editor.fontSize": 17,
    "redhat.telemetry.enabled": true,
    "vscodeGoogleTranslate.preferredLanguage": "Chinese (Simplified)",
    "markdown-preview-enhanced.previewTheme": "github-light.css",
    "editor.accessibilitySupport": "off",
    "editor.unicodeHighlight.nonBasicASCII": false,
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "git.openRepositoryInParentFolders": "never",
    "[python]": {
        "editor.formatOnType": true
    },
    "hediet.vscode-drawio.resizeImages": null,
    "editor.indentSize": "tabSize",
    // vim的设置，norecmap, imap, noremap
    "vim.commandLineModeKeyBindingsNonRecursive": [],
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "j",
                "j"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        // 缩进
        {
            "before": [
                "<Tab>"
            ],
            "after": [
                "V",
                ">"
            ]
        },
        {
            "before": [
                "<S-Tab>"
            ],
            "after": [
                "V",
                "<"
            ]
        },
        // 窗口切换
        {
            "before": [
                "<C-h>"
            ],
            "after": [
                "<C-w>",
                "h"
            ]
        },
        {
            "before": [
                "<C-j>"
            ],
            "after": [
                "<C-w>",
                "j"
            ]
        },
        {
            "before": [
                "<C-k>"
            ],
            "after": [
                "<C-w>",
                "k"
            ]
        },
        {
            "before": [
                "<C-l>"
            ],
            "after": [
                "<C-w>",
                "l"
            ]
        },
        // 移动光标到行首行尾
        {
            "before": [
                "H"
            ],
            "after": [
                "^"
            ]
        },
        {
            "before": [
                "L"
            ],
            "after": [
                "$"
            ]
        },
        // 注释
        {
            "before": [
                "<leader>",
                "c"
            ],
            "after": [
                "g",
                "c",
                "c"
            ]
        },
        // 格式化
        {
            "before": [
                "<leader>",
                "f"
            ],
            "after": [
                "g",
                "g",
                "=",
                "G",
                "<C-o>"
            ]
        },
        // 快速移动光标
        {
            "before": [
                "J"
            ],
            "after": [
                "5",
                "j"
            ]
        },
        {
            "before": [
                "K"
            ],
            "after": [
                "5",
                "k"
            ]
        },
        {
            "before": [
                "j"
            ],
            "after": [
                "g",
                "j"
            ]
        },
        {
            "before": [
                "k"
            ],
            "after": [
                "g",
                "k"
            ]
        },
        // 下一个光标位置
        {
            "before": [
                "<C-p>"
            ],
            "after": [
                "<C-i>"
            ]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        // 缩进
        {
            "before": [
                "<Tab>"
            ],
            "after": [
                ">",
                "g",
                "v"
            ]
        },
        {
            "before": [
                "<S-Tab>"
            ],
            "after": [
                "<",
                "g",
                "v"
            ]
        },
        {
            "before": [
                "<leader>",
                "c"
            ],
            "after": [
                "g",
                "c"
            ]
        },
        // 移动光标到行首行尾
        {
            "before": [
                "H"
            ],
            "after": [
                "^"
            ]
        },
        {
            "before": [
                "L"
            ],
            "after": [
                "$"
            ]
        },
        // 快速移动光标
        {
            "before": [
                "J"
            ],
            "after": [
                "5",
                "j"
            ]
        },
        {
            "before": [
                "K"
            ],
            "after": [
                "5",
                "k"
            ]
        },
        {
            "before": [
                "<C-c>"
            ],
            "after": [
                "\"*y"
            ]
        }
    ],
    "vim.useSystemClipboard": false,
    "arduino.useArduinoCli": true,
    "arduino.path": "D:\\Tool\\other_command",
    "C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: Google, UseTab: Never, IndentWidth: 4, TabWidth: 4 }",
    "latex-workshop.latex.tools": [
        {
            "name": "pdflatex",
            "command": "pdflatex",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "%DOCFILE%"
            ]
        },
        {
            "name": "bibtex",
            "command": "bibtex",
            "args": [
                "%DOCFILE%"
            ]
        },
        {
            "name": "xelatex",
            "command": "xelatex",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "%DOC%"
            ]
        },
    ],
    // 编译策略
    "latex-workshop.latex.recipes": [
        {
            "name": "xelatex",
            "tools": [
                "xelatex"
            ]
        },
        {
            "name": "xelatex -> bibtex -> xelatex*2",
            "tools": [
                "xelatex",
                "bibtex",
                "xelatex",
                "xelatex"
            ]
        }
    ],
    "editor.wordWrap": "on",
    "fittencode.languagePreference.displayPreference": "zh-cn",
    "fittencode.languagePreference.commentPreference": "zh-cn",
    "STM32VSCodeExtension.cubeCLT.path": "d:\\Tool\\STM32CubeCLT_1.15.1",
    "STM32VSCodeExtension.projectCreator.executablePath": "d:\\Tool\\STM32CubeMX\\STM32CubeMX.exe",
    "STM32VSCodeExtension.productFinder.executablePath": "d:\\Tool\\STMCUFinder\\STMCUFinder.exe",
    "godotTools.editorPath.godot4": "D:\\Tools\\Godot\\Godot_v4.3-stable_win64.exe",
    "terminal.integrated.commandsToSkipShell": [
        "matlab.interrupt"
    ],
    "python.languageServer": "Pylance",
    "powermode.shake.enabled": false,
    "powermode.combo.location": "editor",
    "powermode.combo.counterEnabled": "show",
    "powermode.combo.timeout": 5,
    "powermode.combo.timerEnabled": "show",
    "powermode.enabled": true,
    "chat.commandCenter.enabled": false,
    "vscode_custom_css.imports": [],
    "animations.Install-Method": "Custom UI Style",
    "custom-ui-style.external.imports": [],
    "animations.CursorAnimation": true,
    "animations.UseCursorColorForCursorAnimation": true,
    "winopacity.opacity": 248,
    "ltex.ltex-ls.path": "D:\\Tools\\ltex-ls",
    "cSpell.userWords": [
        "rtype",
    ],
    "editor.experimentalEditContextEnabled": false,
    "python.defaultInterpreterPath": "d:\\Python\\Python312\\python.exe",
    "extensions.ignoreRecommendations": true,
    "gitlens.views.scm.grouped.views": {
        "commits": true,
        "branches": true,
        "remotes": true,
        "stashes": true,
        "tags": true,
        "worktrees": true,
        "contributors": true,
        "fileHistory": false,
        "repositories": true,
        "searchAndCompare": false,
        "launchpad": false
    },
}
